# 피보나치 횟수 출력 시간복잡도, 다이나믹 
def one(n):
  if n==0:
    return 0
  if n==1:
    return 1
  return one(n-1)+one(n-2)

def zero(n):
  if n==0:
    return 1
  elif n==1:
    return 0
  elif n==2:
    return 1
  else:
    return zero(n-1)+zero(n-2)

T=int(input())
arr=[]
for _ in range(T):
  N=int(input())
  arr.append(zero(N))
  arr.append(one(N))
for i in range(T):
  print(arr[2*i],arr[2*i+1])
